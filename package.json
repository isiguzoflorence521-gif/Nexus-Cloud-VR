{
  "name": "nexus-cloud-vr",
  "version": "1.1.0",
  "author": "Nexus VR Team",
  "description": "Virtual Console OS for Feebz VR",
  "main": "main.js",
  "scripts": {
    "dist": "electron-builder --win --linux"
  },
  "build": {
    "appId": "com.nexus.vr",
    "productName": "Nexus Cloud VR",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}
